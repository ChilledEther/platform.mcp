name: Go CI
on: [push, pull_request]
jobs:
  build:
    name: Build {{.ProjectName}}
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - name: Set up Go
        uses: actions/setup-go@v5
        with:
          go-version: '1.25'
      - name: Build
        run: go build -v ./...
